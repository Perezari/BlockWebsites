// manifest.json
{
  "manifest_version": 2,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Blocks specified websites",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.koffer24.de/*",
	"*://*.scielo.br/*",
    "tabs"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
